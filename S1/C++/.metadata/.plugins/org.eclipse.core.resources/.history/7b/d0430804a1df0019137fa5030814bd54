#include "../Headers/Papier.h"

const string& Papier::getAuteur() const {
	return auteur;
}

void Papier::setAuteur(const string& auteur) {
	this->auteur = auteur;
}

const string& Papier::getEditeur() const {
	return editeur;
}

void Papier::setEditeur(const string& editeur) {
	this->editeur = editeur;
}
